{
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "https://api.tanish.website/api/v1/:path*"
    },
    {
      "source": "/static/:path*",
      "destination": "https://api.tanish.website/static/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
